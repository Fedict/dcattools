# Fix JPEG2000 file format

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>

DELETE { 
	?dist dcterms:format <http://publications.europa.eu/resource/authority/file-type/JPEG> .
	?dist dcat:mediaType <https://www.iana.org/assignments/media-types/image/jpeg> .
}
INSERT {
	?dist dcterms:format <http://publications.europa.eu/resource/authority/file-type/JPEG2000> .
	?dist dcat:mediaType <https://www.iana.org/assignments/media-types/image/jp2> .
}
WHERE {	
	?dist dcterms:format <http://publications.europa.eu/resource/authority/file-type/JPEG> .
	?dist dcat:mediaType <https://www.iana.org/assignments/media-types/image/jpeg> .
	?dist dcat:mediaType <https://www.iana.org/assignments/media-types/image/jp2> .
	}
	