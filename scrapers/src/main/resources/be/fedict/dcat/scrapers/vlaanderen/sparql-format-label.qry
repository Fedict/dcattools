# Move format label so it can be used for matching in another query

PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

DELETE 
	{	?s dcterms:format ?genfmt .
		?genfmt ?p ?o }
INSERT
	{	?s dcterms:format ?str }
WHERE
	{ 	?s dcterms:format ?genfmt .
		?genfmt skos:prefLabel ?lbl 
		BIND (STR(?lbl) as ?str)
	}
