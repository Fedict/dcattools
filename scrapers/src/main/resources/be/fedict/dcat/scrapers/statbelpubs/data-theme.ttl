# Data for mapping keywords to an EU list of themes/categories

@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<http://publications.europa.eu/resource/authority/data-theme/AGRI>
	skos:exactMatch <http://data.gov.be/en/taxonomy/term/32> ;
	skos:prefLabel "AGRICULTURE, FISHERIES, FORESTRY, FOOD"@en ;
	skos:altLabel "Landbouw & visserij"@nl .

<http://publications.europa.eu/resource/authority/data-theme/ECON> 
	skos:exactMatch <http://data.gov.be/en/taxonomy/term/38> ;
	skos:prefLabel "ECONOMY AND FINANCE"@en ;
	skos:altLabel "Lonen en arbeidskosten"@nl, 
			"Werk & Opleiding"@nl, "Conjunctuurindicatoren"@nl, "Arbeidsmarkt"@nl, "Economie"@nl, "Ondernemingen"@nl .

<http://publications.europa.eu/resource/authority/data-theme/EDUC>
	skos:exactMatch <http://data.gov.be/en/taxonomy/term/33>  ;
	skos:prefLabel "EDUCATION"@en ;
	skos:altLabel "Werk & Opleiding"@nl, "Opleidingen en onderwijs"@nl .

<http://publications.europa.eu/resource/authority/data-theme/ENER>
	skos:exactMatch <http://data.gov.be/en/taxonomy/term/35> ;
	skos:prefLabel "ENERGY"@en ;
	skos:altLabel "Energie"@nl .

<http://publications.europa.eu/resource/authority/data-theme/ENVI>
	skos:exactMatch <http://data.gov.be/en/taxonomy/term/34> ;
	skos:prefLabel "ENVIRONMENT"@en ;
	skos:altLabel "Leefmilieu"@nl .
	
<http://publications.europa.eu/resource/authority/data-theme/SOCI>
	skos:exactMatch <http://data.gov.be/en/taxonomy/term/39> ;
	skos:prefLabel "POPULATION AND SOCIAL CONDITIONS"@en ;
	skos:altLabel "Lonen en arbeidskosten"@nl, "Bevolking"@nl, "Werk"@nl, "Armoede en levensomstandigheden"@nl .

<http://publications.europa.eu/resource/authority/data-theme/TRAN>
	skos:exactMatch  <http://data.gov.be/en/taxonomy/term/36> ;
	skos:prefLabel "TRANSPORT"@en ;
	skos:altLabel "Voertuigenpark"@nl, "Verkeer"@nl, "Mobiliteit"@nl .