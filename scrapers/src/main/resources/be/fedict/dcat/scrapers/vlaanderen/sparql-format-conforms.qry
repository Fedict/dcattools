# Guess file format based on conformance level

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>

INSERT
	{ 	?dist dcterms:format "WMS" }
WHERE
	{
		?dist dcterms:conformsTo ?std .
		?std a dcterms:Standard .
		?std dcterms:identifier "OGC:WMS-1.3.0-http-get-map" .
		FILTER NOT EXISTS { ?dist dcterms:format ?fmt } 
	}
	