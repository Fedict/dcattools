	skos:altLabel "unknown", 
		"http://publications.europa.eu/resource/authority/frequency/UNKNOWN" .

<http://data.gov.be/en/taxonomy/term/390>
	skos:exactMatch <http://publications.europa.eu/resource/authority/frequency/NEVER> ;
	skos:prefLabel "Never"@en ;
