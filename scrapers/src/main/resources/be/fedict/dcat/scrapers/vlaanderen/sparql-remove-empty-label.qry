# Delete format that references a class with only empty skos labels

PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

DELETE 
	{	?s dcterms:format ?genfmt .
		?genfmt skos:prefLabel ?lbl .
		?genfmt ?p ?o }
WHERE
	{ 	?s dcterms:format ?genfmt .
		?genfmt ?p ?o .
		?genfmt skos:prefLabel ?lbl .
		FILTER NOT EXISTS {
			?genfmt skos:prefLabel ?lbl .
			FILTER(STRLEN(?lbl) > 0)
		}
	}
