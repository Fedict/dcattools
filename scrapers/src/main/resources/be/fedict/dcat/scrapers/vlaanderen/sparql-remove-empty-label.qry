# Delete format and frequency that references a class with only empty skos labels

PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

DELETE 
	{	?s ?rel ?genfmt .
		?genfmt skos:prefLabel "" .
		?genfmt ?p ?o }
WHERE
	{ 	?s ?rel ?genfmt .
		?genfmt skos:prefLabel "" .
		?genfmt ?p ?o
		VALUES ?rel { dcterms:format dcterms:accrualPeriodicity }
	}
