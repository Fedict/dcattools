# Convert relative path to absolute IRI

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>

DELETE
	{	?dist dcat:downloadURL ?path }
INSERT 
	{ 	?dist dcat:downloadURL ?uri }
WHERE
	{	?dist dcat:downloadURL ?path .
		?dist dcterms:format ?fmt
		BIND (IRI(
			CONCAT("https://belgianfoodtruckfederation.org/fr/open-data/dwnld/csv/", ?fmt))
		as ?uri)
	}