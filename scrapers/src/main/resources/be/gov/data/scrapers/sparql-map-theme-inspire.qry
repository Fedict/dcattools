# Map an INSPIRE or GEMET theme to one or more DCAT-AP themes

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

DELETE
	{	?s dcat:theme ?uri }
INSERT
	{ 	?s dcat:theme ?theme .
		?s dcterms:subject ?uri }
WHERE
	{ 	
		?s dcat:theme ?uri .
		?theme skos:narrowMatch ?uri .
	}