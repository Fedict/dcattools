# Map an INSPIRE theme to one or more DCAT-AP themes

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

DELETE
	{	?dataset dcat:theme ?inspire }
INSERT
	{ 	?dataset dcat:theme ?theme .
		?dataset dcterms:subject ?inspire }
WHERE
	{ 	
		?dataset dcat:theme ?inspire .
		?inspire skos:broadMatch ?theme .
	}