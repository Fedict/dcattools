# Set publisher and contactpoint

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>

INSERT
	{ 	?dataset dcat:contactPoint <http://data.gov.be/org/fpp/contact> .
		?dataset dcterms:publisher <https://org.belgif.be/id/CbeRegisteredEntity/0254013997> }
WHERE
	{ 	?dataset a dcat:Dataset  }