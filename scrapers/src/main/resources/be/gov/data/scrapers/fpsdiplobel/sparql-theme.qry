# Set category / theme

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>

INSERT 
	{ 	?dataset dcat:theme <http://publications.europa.eu/resource/authority/data-theme/INTR> .
		?dataset dcat:keyword "Internationaal"@nl, "International"@fr, "International"@en }
WHERE
	{	?dataset a dcat:Dataset }