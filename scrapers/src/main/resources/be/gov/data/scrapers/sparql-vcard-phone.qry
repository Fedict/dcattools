# Fix invalid phone: literal (spaces and international number)

PREFIX vcard: <http://www.w3.org/2006/vcard/ns#>

DELETE 
	{ ?s vcard:hasTelephone ?val }
INSERT 
	{ ?s vcard:hasTelephone ?iri }
WHERE 
	{ ?s vcard:hasTelephone ?val .
		BIND (
			IRI(
				CONCAT("tel:", 
						REPLACE(
							REPLACE(STR(?val), " ", "")
						, "^(tel:)?(\\+)?(32)?0" ,"+32")
					)
			) 
		as ?iri)
	}
