# Fix incorrect use of dct:type on license

PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX vcard: <http://www.w3.org/2006/vcard/ns#>

DELETE {
		?s dcterms:license ?lic .
		?lic dcterms:type ?uri .
		?lic ?p ?o
	}
INSERT 
	{	?s dcterms:license ?uri }
WHERE {
		?s dcterms:license ?lic .
		?lic dcterms:type ?uri .
		OPTIONAL { ?lic ?p ?o }
	}
