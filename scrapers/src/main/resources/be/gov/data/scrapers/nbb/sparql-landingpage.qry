# Add landingpage based on accessURL, if missing

PREFIX dcat: <http://www.w3.org/ns/dcat#>

INSERT
	{ 	?dataset dcat:landingPage ?url }
WHERE
	{ 	?dataset a ?cl .
		?dataset dcat:accessURL ?url 
		VALUES ?cl { dcat:Dataset dcat:DataService }
		FILTER NOT EXISTS  { ?dataset dcat:landingPage ?page }
		}