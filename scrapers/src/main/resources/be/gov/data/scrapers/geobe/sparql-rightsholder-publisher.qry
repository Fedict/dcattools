# Set publisher from rightsholder if not present

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>

INSERT
	{	?dataset dcterms:publisher ?r }
WHERE
	{ 	?dataset a ?cl .
		?dataset ?prop ?r .
		VALUES ?prop { dcterms:publisher <http://data.europa.eu/930/custodian> }
		VALUES ?cl { dcat:Dataset dcat:DataService	}
		FILTER NOT EXISTS { ?dataset dcterms:publisher ?o }
	}