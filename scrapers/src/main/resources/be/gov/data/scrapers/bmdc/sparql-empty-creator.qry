# Remove empty creator

PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

DELETE {
		?s dcterms:creator ?skolem .
		?skolem a foaf:Agent .
		?skolem a foaf:Organization .
	}
WHERE
	{	?s dcterms:creator ?skolem .
		?skolem a foaf:Organization .
		OPTIONAL { ?skolem a foaf:Agent }.
		FILTER NOT EXISTS { ?skolem foaf:name ?name }
	}