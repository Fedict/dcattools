# Use accessURL of the dataset if the access URL of the distribution is missing

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX adms: <http://www.w3.org/ns/adms#>

INSERT
	{ 	?dist dcat:accessURL ?url }
WHERE
	{ 	?dataset dcat:accessURL ?url .
		?dataset ?p ?dist .
		?dist a dcat:Distribution
		VALUES ?p { adms:sample dcat:distribution }
		FILTER NOT EXISTS { ?dist dcat:accessURL ?o }
	}
