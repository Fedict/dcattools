# Map publisher for datasets from Ghent to Crossroad Bank Enterprises URI

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>

INSERT 
	{ 	?dataset dcterms:publisher <https://org.belgif.be/id/CbeRegisteredEntity/0207451227> }
WHERE
	{	?dataset a dcat:Dataset
		FILTER (STRSTARTS(STR(?dataset), "https://data.stad.gent"))
		FILTER NOT EXISTS { ?dataset dcterms:publisher ?publ }
	}