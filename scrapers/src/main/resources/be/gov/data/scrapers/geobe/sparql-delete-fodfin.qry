# Remove fodfin dataset since the 5000+ distributions make it hard to process for the EDP

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>

DELETE
	{	?dataset dcat:distribution ?dist .
		?dist a dcat:Distribution .
		?dist dcat:downloadURL ?url .
		?dist ?p ?o 
		}
WHERE
	{ 	?dataset dcat:distribution ?dist .
		?dist a dcat:Distribution .
		?dist dcat:downloadURL ?url .
		?dist ?p ?o 
		FILTER (CONTAINS(STR(?url), "https://opendata.fin.belgium.be/")) }
