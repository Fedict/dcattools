# Set datatype on dcat:temporalResolution

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

DELETE	{	?s ?p ?o }
INSERT 	{	?s ?p ?dtype }
WHERE	{
			?s dcat:temporalResolution ?o .
			BIND(
				STRDT(STR(?o), xsd:duration)
				AS ?dtype
			)
			FILTER ( datatype(?o) = xsd:string )
}